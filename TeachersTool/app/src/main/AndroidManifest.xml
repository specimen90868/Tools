            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.teamtools.teacherstool.MainActivity" />
        </activity>
        <activity
            android:name=".frmCriterios"
            android:label="@string/title_activity_frm_criterios"
            android:parentActivityName=".catCriterios"
