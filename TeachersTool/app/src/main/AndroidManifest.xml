            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.teamtools.teacherstool.catCriterios" />
        </activity>
        <activity
            android:name=".catMaterias"
            android:label="@string/title_activity_cat_materias"
